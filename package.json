{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "check:server-no-ts": "node ./scripts/check_no_ts_in_server.mjs",
    "remove:server-ts": "node ./scripts/remove_server_ts_files.mjs",
    "clean:server-ts": "node ./scripts/clean_server_ts.mjs",
    "check:env:local": "node scripts/check_env_local.cjs",
    "dev": "npm run preclean:components && npm run check:env:local && vite",
    "dev:full": "node ./scripts/dev-full.mjs",
    "dev:vercel": "vercel dev",
    "create-demo-users": "node scripts/create_demo_users.js",
    "preclean:components": "rm -f src/components/*.js",
    "build": "npm run preclean:components && vite build",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "scan:placeholders": "node scripts/scan_placeholders.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
  "test:integration": "vitest run --config tests/vitest.integration.config.ts",
    "test:unit": "vitest run src/services/__tests__/batchService.test.ts",
    "env:check": "node scripts/check_vite_api.cjs",
    "env:toggle": "sh scripts/toggle_dev_env.sh",
    "test:e2e": "playwright test -c tests/e2e/playwright.config.ts",
    "test:coverage": "vitest run --coverage",
    "preview": "vite preview",
    "reset:demo": "node scripts/reset_demo_data.js",
    "deploy:vercel": "vercel",
    "deploy:vercel:prod": "vercel --prod",
    "prestart:server": "node scripts/check_ports.cjs",
    "start:server": "npm run prestart:server && node server/index.js",
    "start:server:e2e": "E2E_TEST_MODE=true node server/index.js",
    "import:courses": "node scripts/import_courses.js import/courses-template.json",
    "import:courses:publish": "node scripts/import_courses.js import/courses-template.json --publish",
    "audit": "npm audit --audit-level moderate",
    "audit:fix": "npm audit fix",
    "analyze": "npx vite-bundle-analyzer",
    "check:bundle": "node scripts/check_bundle_size.mjs",
    "ci": "npm run -s typecheck && npm run -s lint && vitest run",
    "archive:server-ts": "node scripts/archive_server_ts.mjs",
    "migrate:local": "sh ./scripts/run_migration.sh supabase/migrations/20251108_add_analytics_tables_and_views.sql",
    "seed:analytics": "node ./scripts/seed_analytics.js",
    "diag:env": "node scripts/check_deploy_env.cjs",
    "diag:print": "node scripts/print_vite_env.cjs",
    "diag:ssl": "node scripts/check_ssl.cjs",
    "optimize:build": "npm ci && npm run build && npm run analyze",
    "scan:dist": "node scripts/find_hosts_in_dist.mjs",
    "check:lock": "node scripts/check_lock_sync.mjs"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@supabase/supabase-js": "^2.86.0",
    "@tanstack/react-query": "^5.90.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.10",
    "@types/crypto-js": "^4.2.2",
    "@types/csurf": "^1.11.5",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^7.0.3",
    "@types/speakeasy": "^2.0.10",
    "bcryptjs": "^3.0.3",
    "chart.js": "^4.5.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "csurf": "^1.2.2",
    "dompurify": "^3.3.0",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "framer-motion": "^12.23.24",
    "helmet": "^8.1.0",
    "hls.js": "^1.4.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.344.0",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.10",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "react": "18.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^7.9.5",
    "recharts": "^3.2.1",
    "speakeasy": "^2.0.0",
    "uuid": "^13.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@axe-core/cli": "^4.10.2",
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.19",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.0",
    "@types/pg": "^8.15.6",
    "@types/react": "^18.3.27",
    "@types/react-dnd-html5-backend": "^3.0.2",
    "@types/react-dom": "^18.3.7",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^27.0.0",
    "playwright": "^1.38.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.1",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.2.4",
    "vite-bundle-analyzer": "^1.2.3",
    "vitest": "^3.2.4",
    "ws": "^8.13.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
