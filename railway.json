{
  "project": "MainProject",
  "services": [
    {
      "name": "api",
      "provider": "node",
      "rootDirectory": "./",
      "start": "npm run start:server",
      "build": "npm run build:server",
      "env": [
        "SUPABASE_URL",
        "SUPABASE_SERVICE_ROLE_KEY",
        "SUPABASE_ANON_KEY",
        "CORS_ALLOWED_ORIGINS",
        "PORT",
        "DEV_FALLBACK",
        "JWT_ACCESS_SECRET",
        "JWT_REFRESH_SECRET",
        "JWT_REFRESH_SECRET",
        "COOKIE_DOMAIN"
      ]
    },
    {
      "name": "frontend",
      "provider": "static",
      "rootDirectory": "./",
      "build": "npm run build:client",
      "publish": "dist",
      "env": [
        "VITE_API_BASE_URL",
        "VITE_WS_URL",
        "VITE_ENABLE_WS",
        "VITE_SUPABASE_URL",
        "VITE_SUPABASE_ANON_KEY"
      ]
    }
  ]
}
